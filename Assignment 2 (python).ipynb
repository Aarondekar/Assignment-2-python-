   "source": [
    "#Create the below pattern using nested for loop in python\n",
    " *\n",
    " **\n",
    " ***\n",
    " ****\n",
    " *****\n",
    " ****\n",
    " ***\n",
    " **\n",
    " *"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "*\n",
      "**\n",
      "***\n",
      "****\n",
      "*****\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n"
     ]
    }
   ],
   "source": [
    "s=\"*\"\n",
    "g=\"******\"\n",
    "for i in g:\n",
    "    if s!=g:\n",
    "        print(s)\n",
    "        s=s+\"*\"\n",
    "    else:\n",
    "        k=4\n",
    "        for i in s:\n",
    "            print(s[0:k])\n",
    "            k=k-1\n",
    "            if k==0:\n",
    "                break"
   ]
  },
